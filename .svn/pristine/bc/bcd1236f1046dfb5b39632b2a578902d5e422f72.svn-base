.BILL {
  width: 100%;
}
.BILL .title {
  color: #919699;
  font-size: 18px;
  padding: 15px 0 25px 15px;
  margin: 0;
}
.BILL .earnList {
  width: 100%;
}
.BILL .earnList ul {
  overflow: hidden;
  margin: 0 0 10px 0;
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.BILL .earnList ul li {
  width: 218px;
  min-height: 80px;
  border-radius: 4px;
  padding: 20px 0 0;
  border: 1px solid #e8e9eb;
  margin-bottom: 10px;
  text-align: center;
}
.BILL .earnList ul li p {
  line-height: 25px;
}
.BILL .earnList ul li .listTilte {
  font-size: 16px;
}
.BILL .earnList ul li .listMoney {
  color: #ed8e0e ;
  font-size: 22px;
}
.BILL .earnList ul li .listMoney .moneyIcon {
  font-size: 16px;
}
.BILL .earnList ul li .listAccount {
  font-size: 14px;
  color: #919699;
}
.BILL .calendar {
  width: 100%;
  overflow: hidden;
}
.BILL .calendar .calendarS {
  float: left;
  width: 130px;
  height: 30px;
  line-height: 30px;
  border: 1px solid #e8e9eb;
  border-radius: 4px;
  overflow: hidden;
  margin-bottom: 10px;
}
.BILL .calendar .calendarS input {
  border: none;
  width: 110px;
  height: 28px;
  float: left;
  margin: 0;
  box-shadow: none;
}
.BILL .calendar .calendarS i {
  float: left;
  margin-top: 5px;
  color: #e8e9eb;
}
.BILL .calendar .line {
  float: left;
  margin: 0 5px;
}
.BILL .calendar .search {
  float: left;
  border-radius: 4px;
  margin: 0 10px;
  width: 35px;
  height: 30px;
  text-align: center;
  line-height: 30px;
  background-color: #bfe7ff;
  cursor: pointer;
}
.BILL .calendar .search i {
  color: #fff ;
}
.BILL .calendar .time {
  float: left;
  min-width: 100px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
}
.BILL .calendar .searchinfo {
  float: left;
  width: 160px;
  height: 30px;
  border-radius: 4px;
  border: 1px solid #e8e9eb;
  margin-left: 10px;
  margin-bottom: 10px;
  font-size: 14px;
  color: #e8e9eb;
}
.BILL .calendar .searchinfo input {
  border: none;
  width: 100%;
  height: 28px;
  padding: 0 5px;
  border-radius: 4px;
  font-size: 14px;
  box-shadow: none;
}
.BILL .calendar .selects {
  width: 120px;
  height: 30px;
}
.BILL .calendar .selects select {
  width: 100%;
  height: 28px;
  border: none;
  padding: 0 5px;
  border-radius: 4px;
  font-size: 14px;
}
.table {
  border-left: none;
  border-right: none;
  border-top: none;
  vertical-align: middle;
}
.table thead {
  background-color: #fff;
  border-top: none;
}
.table th,
.table td,
.table tr {
  text-align: center;
  vertical-align: middle;
}
.table .rechargeDetail a {
  display: inline-block;
  width: 100px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  color: #22a3f4;
  border: 1px solid #22a3f4;
  margin: 0 10px;
  border-radius: 4px;
}
.userSate table tr td {
  padding: 15px 10px;
}
.channelState .container-fluid {
  width: 100%;
  padding: 0;
}
.channelState .container-fluid .row {
  width: 100%;
  max-width: none;
}
.channelState .col-md-12 {
  width: 100%;
  padding: 0;
}
.channelState .col-md-12 .nav {
  margin: 0;
  padding: 0;
}
.channelState .col-md-12 .nav li a {
  color: #5c6266 ;
  padding: 15px 30px;
  font-size: 18px;
}
.channelState .col-md-12 .nav li:first-child a {
  border-left-color: transparent;
}
.channelState .col-md-12 .nav .active a {
  color: #22a3f4;
  border-top: 2px solid #22a3f4;
}
.channelState .searching {
  overflow: hidden;
  margin: 10px 0;
  padding-left: 20px;
}
.channelState .searching input {
  float: left;
  width: 250px;
  height: 32px;
  border: 1px solid #ddd ;
  border-radius: 4px;
  box-shadow: none;
  margin: 0;
}
.channelState .searching .searchbtn {
  width: 40px;
  height: 32px;
  float: left;
  line-height: 32px;
  border-radius: 4px;
  background-color: #bfe7ff;
  text-align: center;
  margin: 0 10px;
  cursor: pointer;
}
.channelState .searching .searchbtn i {
  color: #fff;
  font-size: 14px;
}
.channelState tbody .cost input {
  width: 80px;
  height: 28px;
  border: 1px solid #ddd ;
  border-radius: 4px;
  margin: 0;
  text-align: center;
}
.table tbody tr:hover {
  background-color: #f2f6fa;
}
.table tbody tr td {
  vertical-align: middle;
}
