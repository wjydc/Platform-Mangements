
// 公用

.CashRegister{
    width:100%;
    .title{
    color: #919699;
    font-size:18px;
    padding: 15px 0 15px 15px;
    margin: 0;
    }
    // 提现列表
    .CashList{
        width:100%;
        .CashListul{
            overflow: hidden;
           margin:0 0 10px 0;
           
          .CashListulli{
                width:218px;
                min-height:80px;
                border-radius:4px;
                padding: 20px 0 0;
                border:1px solid #e8e9eb;
                margin-bottom: 10px;
                text-align: center;
                float: left;
                margin-right: 50px;
                p{
                    line-height:25px;
                    margin-bottom: 10px;
                }
                .listTilte{
                    font-size: 16px;
                }
                .listMoney{
                    color: #ed8e0e ;
                    font-size: 22px;
                     margin-bottom: 20px;
                    .moneyIcon{
                        font-size: 16px;
                    }
                }
                .listAccount{
                    font-size: 14px;
                    color:#919699;
                }
            }
            .CashNote{
                width:218px;
                min-height:80px;
                float: left;
               
                .listTilte{
                     margin:0 0 10px 0;
                }
                p{
                  color:#bbbebf;
                 font-size: 12px;
                
                 
                }
                ul{
                    margin:0;
                    li{
                    color:#bbbebf;
                     font-size:12px;
                     overflow: hidden;
                     margin-bottom:5px;
                     .circle{
                        display:inline-block;
                        width:3px;
                        height: 3px;
                        background-color:#22a3f4;
                        margin-top:8.5px;
                        margin-right: 5px;
                        position: relative;
                        float: left;

                    }
                     span:last-child{
                         font-size:12px;
                     }
                    
                  }
                }
               
              
            }
        }
    }
    .ApplyCash{
        width:180px;
        height:40px;
        background-color: #22a3f4;
        border-radius: 4px;
        text-align: center;
        line-height: 40px;
        margin:0 0 30px;
        color:#fff;
        font-size: 18px;
        cursor: pointer;
        i{
            font-size: 16px;
             color:#fff;
             margin-right: 5px;
        }
    }
    .calendar{
        width:100%;
        overflow: hidden;
        .calendarS{
            float: left;
            width:180px;
            height:30px;
            line-height: 30px;
            border:1px solid #e8e9eb;
            border-radius: 4px;
            overflow: hidden;
            margin-bottom: 10px;
            input{
                border:none;
                width:175px;
                height:28px;
                float: left;
                margin:0;
                box-shadow: none;
               text-align: center;
                outline: none;
            }
            i{
                float: left;
                margin-top: 5px;
                color:#e8e9eb;
            }
        }
        .line{
                float: left;
                margin:0 5px;
        }
        .search{
            float: left;
            border-radius: 4px;
            margin:0 10px;
           width:35px;
           height:30px;
           text-align: center;
           line-height: 30px;
           background-color: #bfe7ff;
           cursor: pointer;
           i{
               color:#fff ;
           }
        }
        .time{
            float: left;
            min-width: 100px;
            height: 30px;
            line-height: 30px;
            font-size: 14px;
            
        }
       
        .searchinfo{
            float: left;
            width:160px;
            height:30px;
            border-radius: 4px;
            border:1px solid #e8e9eb;
            margin-left:10px;
            margin-bottom: 10px;
            font-size: 14px;
            // color: #e8e9eb;
        
            input{
                border:none;
                width:100%;
                height:28px;
                padding:0 5px;
                border-radius: 4px;
                font-size: 14px;
                box-shadow: none;
            }
        }
        .selects{
            width:120px;
            height:30px;
            select{
                width:100%;
                height:28px;
                border:none;
                 padding:0 5px;
                border-radius: 4px;
                 font-size: 14px;
            }
        }
    }
}
